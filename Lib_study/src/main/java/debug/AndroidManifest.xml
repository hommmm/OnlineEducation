<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.study">

    <application
        android:name="debug.StudyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/study_icon"
        android:label="@string/study_name"
        android:roundIcon="@mipmap/study_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineEducation"
        tools:ignore="UnusedAttribute"
        tools:replace="android:label,android:theme">

        <activity android:name="com.example.study.StudyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>